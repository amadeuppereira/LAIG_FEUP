    <views default="view1" >

        <perspective id="view1" near="0.1" far="500" angle="23">
            <from x="180" y="180" z="180" />
            <to x="0" y="0" z="0" />
        </perspective>

        <perspective id="view2" near="30" far="500" angle="30">
