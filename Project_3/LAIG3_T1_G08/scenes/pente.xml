        <transformation id="transformationWall">
            <translate x="20" y="9" z="0.25" />
            <scale x="40" y="18" z="0.5" />
        </transformation>

        <transformation id="transformationPortal">
            <translate x="4" y="7" z="0.375" />
            <scale x="8" y="14" z="0.001" />
        </transformation>

        <transformation id="transformationFloor">
            <translate x="0" y="0.1" z="40" />
            <rotate axis="x" angle="-90" />
            <translate x="20" y="20" z="0" />
            <scale x="40" y="40" z="1" />
        </transformation>

        <transformation id="transformationRock">
            <translate x="0" y="3" z="0" />
            <scale x="6" y="6" z="6" />
        </transformation>

    </transformations>

    <animations>
        <linear id="robotAnimation1" span="3">
            <controlpoint x="30" y="0" z="55"/>
            <controlpoint x="15" y="0" z="55"/>
        </linear>
        <linear id="robotAnimation2" span="3">
            <controlpoint x="78" y="0" z="-4"/>
            <controlpoint x="63" y="0" z="-4"/>
        </linear>
        <circular id="robotCircularAnimation" span="3" center="63 0 -4" radius="0" startang="-90" rotang="180"/>
        <linear id="robotAnimation3" span="3">
            <controlpoint x="63" y="0" z="-4"/>
            <controlpoint x="78" y="0" z="-4"/>
        </linear>
        <linear id="robotAnimation4" span="3">
            <controlpoint x="15" y="0" z="55"/>
            <controlpoint x="30" y="0" z="55"/>
        </linear>
        <circular id="shipCircularAnimation" span="25" center="25 20 25" radius="30" startang="180" rotang="-1080"/>
        <linear id="vehicleAnimation1" span="5">
            <controlpoint x="0" y="0" z="0"/>
            <controlpoint x="0.1" y="10" z="0.1"/>
        </linear>
        <linear id="vehicleAnimation2" span="3">
            <controlpoint x="0" y="10" z="0"/>
            <controlpoint x="55" y="10" z="50"/>
        </linear>
        <linear id="vehicleAnimation3" span="3">
            <controlpoint x="55" y="10" z="50"/>
            <controlpoint x="45" y="15" z="-55"/>
        </linear>
        <linear id="vehicleAnimation4" span="1">
            <controlpoint x="-10" y="15" z="0"/>
            <controlpoint x="-10" y="100" z="0"/>
        </linear>
        <linear id="vehicleAnimation5" span="3">
            <controlpoint x="-10" y="100" z="0"/>
            <controlpoint x="30" y="10" z="55"/>
        </linear>
        <circular id="vehicleCircularAnimation1" span="5" center="0 0 0" radius="0" startang="0" rotang="180"/>
        <circular id="vehicleCircularAnimation2" span="2" center="55 10 50" radius="0" startang="0" rotang="360"/>
        <circular id="vehicleCircularAnimation3" span="8" center="45 15 0" radius="55" startang="90" rotang="90"/>
        <circular id="vehicleCircularAnimation4" span="1.5" center="-10 100 0" radius="0" startang="90" rotang="90"/>

    </animations>

    <primitives>

        <primitive id="rectangle">          
            <rectangle x1="-0.5" y1="-0.5" x2="0.5" y2="0.5" />      
        </primitive>

        <primitive id="cylinder">
            <cylinder base="0.5" top="0.5" height="1" slices="20" stacks="20" />
        </primitive>

        <primitive id="ship_body">
            <cylinder base="5" top="3" height="2" slices="30" stacks="30" />
        </primitive>

        <primitive id="ship_torus">
            <torus inner="0.2" outer="3" slices="50" loops="50" />          
        </primitive>

        <primitive id="robot_arm">
            <cylinder base="0.7" top="0.2" height="4" slices="8" stacks="8" />
        </primitive>

        <primitive id="sphere">
            <sphere radius="0.5" slices="50" stacks="20" />
        </primitive>

        <primitive id="sphere_tree">
            <sphere radius="0.5" slices="8" stacks="8" />
        </primitive>

        <primitive id="torus">
            <torus inner="1.5" outer="3" slices="50" loops="50" />
        </primitive>

        <primitive id="triangle">
            <triangle x1="0.5" y1="1" z1="0" x2="0" y2="0" z2="0" x3="1" y3="0" z3="0" />
        </primitive>

        <primitive id="plane">
            <plane npartsU="5" npartsV="2"/>
        </primitive>

        <primitive id="patch">
            <patch npointsU="2" npointsV="2" npartsU="2" npartsV="2" >
                <controlpoint xx="0" yy="1" zz="2" />
                <controlpoint xx="0" yy="2" zz="4" />
                <controlpoint xx="0" yy="3" zz="5" />
                <controlpoint xx="0" yy="4" zz="6" />
            </patch>
        </primitive>

        <primitive id="vehicle">
