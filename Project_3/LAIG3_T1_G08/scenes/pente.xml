        </primitive>

        <primitive id="cylinder2">
            <cylinder2 base="0.5" top="0.5" height="1" slices="20" stacks="20" />
        </primitive>

        <primitive id="terrainIbiza">
            <terrain idtexture="ibizaTexture" idheightmap="ibizaHeightmap" parts="50" heightscale="0.4"/>
        </primitive>

        <primitive id="water">
            <water idtexture="waterTexture" idwavemap="waterHeightmap" parts="50" heightscale="0.005" texscale="10"/>
        </primitive>            

    </primitives >

